#include <iostream>
using namespace std;

//初始化列表

class Person{
public:
	//传统初始化操作
//	Person(int a,int b,int c) {
//		m_A = a;
//		m_B = b;
//		m_C = c;
//	}

	//初始化列表初始化属性
	Person() :m_A(10), m_B(20), m_C(30) {

	}

	//初始化列表初始化属性
	Person(int a,int b,int c) :m_A(a), m_B(b), m_C(c) {

	}

	int m_A;
	int m_B;
	int m_C;
};

void test01() {
	Person p1;
	Person p2(30,20,10);
	cout << p1.m_A << '\n' << p1.m_B << '\n' << p1.m_C << endl;
	cout << p2.m_A << '\n' << p2.m_B << '\n' << p2.m_C << endl;
}

int main() {
	test01();
}
